<?xml version="1.0" encoding="utf-8"?>
<TableRow
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:background="@drawable/tablerowdrawable"
        android:layout_height="wrap_content">
    <TextView
            android:layout_width="80dp"
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:textSize="@dimen/space15"
            android:textAlignment="center"
            android:id="@+id/tv_date_ins"
            android:minLines="3"
            android:background="@drawable/tablerowdrawable"
            android:maxLines="3"
            />
    <TextView
            android:layout_width="125dp"
            android:layout_height="wrap_content"
            android:background="@drawable/tablerowdrawable"
            android:textColor="@color/black"
            android:textSize="@dimen/space15"
            android:textAlignment="center"
            android:minLines="3"
            android:id="@+id/tv_typ_ins"
            android:maxLines="3"
            />
    <TextView
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:background="@drawable/tablerowdrawable"
            android:textColor="@color/black"
            android:textSize="@dimen/space15"
            android:textAlignment="center"
            android:id="@+id/tv_clientname_ins"
            android:minLines="3"
            android:maxLines="3"
            />
    <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="150dp"
            android:minLines="3"
            android:layout_height="wrap_content"
            android:background="@drawable/tablerowdrawable"
            android:textColor="@color/black"
            android:textSize="@dimen/space15"
            android:textAlignment="center"
            android:maxLines="3"
            android:id="@+id/tv_vandor_ins"
            android:ellipsize="end"
            />
    <TextView
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:background="@drawable/tablerowdrawable"
            android:textColor="@color/black"
            android:textSize="@dimen/space15"
            android:textAlignment="center"
            android:id="@+id/tv_po_number_ins"
            android:minLines="3"
            android:maxLines="3"
            />
    <TextView
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:background="@drawable/tablerowdrawable"
            android:textColor="@color/black"
            android:textSize="@dimen/space15"
            android:textAlignment="center"
            android:id="@+id/tv_range_ins"
            android:minLines="3"
            android:maxLines="3"
            />
    <TextView
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:background="@drawable/tablerowdrawable"
            android:textColor="@color/black"
            android:textSize="@dimen/space15"
            android:textAlignment="center"
            android:id="@+id/tv_expenses_ins"
            android:minLines="3"
            android:maxLines="3"
            />
    <TextView
            android:layout_width="170dp"
            android:layout_height="wrap_content"
            android:background="@drawable/tablerowdrawable"
            android:textColor="@color/black"
            android:textSize="@dimen/space15"
            android:textAlignment="center"
            android:id="@+id/tv_report_no_ins"
            android:minLines="3"
            android:maxLines="3"
            />
    <TextView
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:background="@drawable/tablerowdrawable"
            android:textColor="@color/black"
            android:textSize="@dimen/space15"
            android:textAlignment="center"
            android:id="@+id/tv_quantity_ins"
            android:minLines="3"
            android:maxLines="3"
            />
    <TextView
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:background="@drawable/tablerowdrawable"
            android:textColor="@color/black"
            android:textSize="@dimen/space15"
            android:textAlignment="center"
            android:id="@+id/tv_invoice_ins"
            android:minLines="3"
            android:maxLines="3"
            />

</TableRow>